#include <iostream>
using namespace std;
int main() {
    setlocale(LC_ALL, "Rus");
    int A, B;
    cout << "Введите два целых положительных числа A и B" << endl;
    cin >> A >> B;
    while ((A != 0) & (B != 0)) {
        if (A > B) A = A % B;
        else B = B % A;
    }
    cout << A + B;
    return 0;
}
