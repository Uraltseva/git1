#include<iostream>
using namespace std;
int main()
{
    setlocale(LC_ALL, "Rus");
    int A, B;
    cout << "Введите целое число A" << endl;
    cin >> A;
    cout << "Введите целое число B" << endl;
    cin >> B;
    if (A > 2 && B <= 3) {
        cout << "Истинно" << endl;
    }
    else {
        cout << "Ложь" << endl;
    }
    return 0;
}
