#include <iostream>
using namespace std;
int main()
{
    {
        setlocale(LC_ALL, "Rus");
        int N;
        cout << "Введите размер массива" << endl;
        cin >> N;
        int* array = new int[N];
        cout << "Введите элементы массива" << endl;
        for (int j = 0; j < N; j++) {
            cin >> array[j];
        }
        for (int i = 1; i < N; i += 2) { cout << array[i] << " "; }
        for (int i = 0; i < N; i += 2) { cout << array[i] << " "; }
    }
    return 0;
}
