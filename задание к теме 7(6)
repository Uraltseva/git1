#include <iostream>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Rus");
	float A1, A2, B1, B2, C1, C2, x, y, D, D1, D2;
	cout << "введите коэффициенты A1, A2, B1, B2, C1, C2" << endl;
	cout << "A1, A2, B1, B2, C1, C2 = ";
	cin >> A1 >> A2 >> B1 >> B2 >> C1 >> C2;
	D = A1 * B2 - A2 * B1;
	D1 = C1 * B2 - B1 * C2;
	D2 = A1 * C2 - A2 * C1;
	x = D1 / D;
	y = D2 / D; 
	cout << "x равно " << x << endl;
	cout << "y равно " << y << endl;
	return 0;
}
